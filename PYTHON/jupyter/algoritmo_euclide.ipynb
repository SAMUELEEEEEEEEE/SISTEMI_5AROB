   "source": [
    "def algoritmo_di_euclide(n1, n2):\n",
    "    if n2 > n1:\n",
    "        n1, n2 = n2, n1\n",
    "\n",
    "    ris = n2\n",
    "    while ris != 0:\n",
    "        ris = n1 % n2\n",
    "        n2, n1 = ris, n2\n",
    "\n",
    "    return n1"
   ]
